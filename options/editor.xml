<application>
  <component name="CodeInsightSettings">
    <option name="OPTIMIZE_IMPORTS_ON_THE_FLY" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="com.mysema" />
    <EXCLUDED_PACKAGE NAME="com.sun.org.apache.xerces.internal.*" />
    <EXCLUDED_PACKAGE NAME="junit.framework.Assert" />
    <EXCLUDED_PACKAGE NAME="junit.framework.TestCase" />
    <EXCLUDED_PACKAGE NAME="net.bytebuddy.matcher.ElementMatchers" />
    <EXCLUDED_PACKAGE NAME="nl.jqno.equalsverifier.internal.lib.bytebuddy.matcher.ElementMatchers" />
    <EXCLUDED_PACKAGE NAME="org.apache.webbeans.util.Asserts" />
    <EXCLUDED_PACKAGE NAME="org.hamcrest.core.Is" />
    <EXCLUDED_PACKAGE NAME="org.hibernate.validator.internal" />
    <EXCLUDED_PACKAGE NAME="org.mockito.MockitoAnnotations" />
    <EXCLUDED_PACKAGE NAME="sun.nio" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="references" />
      </set>
    </option>
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value=".references" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>